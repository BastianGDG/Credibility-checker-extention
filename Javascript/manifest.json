{
    "manifest_version": 3,
    "name": "KildeTjekker",
    "version": "1.0",
    "description": "Tjekker om markeret tekst er understøttet af kilder via AI eller Google-søgning",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "contextMenus", "scripting", "storage"]
  }
  